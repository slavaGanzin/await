#!/usr/bin/env bash
./build
echo """# await
*runs list of commands and waits for their termination*

## Installation
Just download await binary from here https://github.com/slavaGanzin/await/releases/tag/0.9

## You can
### Healthcheck FAANG
<img src='demo1.gif' width='100%'/>



## --help
\`\`\`bash
$(./await --help)

\`\`\`
""" > README.md
git add README.md
cp await /usr/local/bin/await
